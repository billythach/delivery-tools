        <addForeignKeyConstraint baseColumnNames="plateform_id"
                                 baseTableName="deploy_command_line"
                                 constraintName="fk_deploy_command_line_plateform_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plateform"/>

    </changeSet>
</databaseChangeLog>
